MaxReverseSpeed = -5
BrakingDeceleration = 300
NoControlDeceleration = 150
*/

/* movement */

TurnAngle = phy_speed
if TurnAngle &gt; MaxTurnAngle {
    TurnAngle = MaxTurnAngle
}

switch state_ {
    case MOVING:
        if phy_speed == 0 {
            state_ = STOP
            break
        }
        if keyboard_check(ord('W')) {
            physics_apply_local_force(WheelX,WheelY,0,Acceleration)
        }
        /*if (keyboard_check(ord('A')) &amp;&amp; !keyboard_check(ord('W')) &amp;&amp; !keyboard_check(ord('S'))) {
            phy_rotation = phy_rotation - TurnAngle 
            physics_apply_local_force(WheelX,WheelY,0,TotalForwardImpulse)
            break
        }
        if (keyboard_check(ord('D')) &amp;&amp; !keyboard_check(ord('W')) &amp;&amp; !keyboard_check(ord('S'))) {
            TotalForwardImpulse = TotalForwardImpulse - NoImpulseDeceleration
            phy_rotation = phy_rotation + TurnAngle 
            physics_apply_local_force(WheelX,WheelY,0,Acceleration)
            break
        }*/
        if keyboard_check(ord('A')) {
            phy_rotation = phy_rotation - TurnAngle
            physics_apply_local_force(WheelX,WheelY,0,Acceleration)
        }
        if keyboard_check(ord('D')) {
            phy_rotation = phy_rotation + TurnAngle
            physics_apply_local_force(WheelX,WheelY,0,Acceleration)
        }
        if keyboard_check(ord('S')) {
            if phy_speed &gt; 0 {
                physics_apply_local_force(WheelX,WheelY,0,BrakingDeceleration)
            }
            if phy_speed &lt; 0 {
                physics_apply_local_force(WheelX,WheelY,0,-Acceleration)
            }
        }
        break
    case STOP:
        if keyboard_check(ord('W')) {
            state_ = MOVING
            physics_apply_local_force(WheelX,WheelY,0,Acceleration) 
        }
        if keyboard_check(ord('S')) {
            state_ = MOVING
            physics_apply_local_force(WheelX,WheelY,0,-Acceleration)
        }
        break
        
}
</string>
